}

img {
	border: none;
}

.fixSelectWidthWithIcon {
	width: 134px;
}

.fixSelectWidth {
	width: 150px;
}

.dynamicSelectWidth {
	width: 80%;
}

.fixWideSelectWidth {
	width: 300px;
}

.fixMiddleSelectWidth {
	width: 220px;
}

.expandText {
	width: 98%;
}

.expandTextArea {
	width: 98%;
}

.expandTextAreaWH {
	width: 98%;
	height: 99%;
}

.inputText {
}

textarea {
