<table class="table-field">
	<thead>
		<tr class="table-field-row">
			#foreach($label in $header)
				<th class="table-field-header">$label</th>
			#end
		</tr>
	</thead>
	<tbody>
		#foreach($row in $rows)
			<tr class="table-field-row">
				#foreach($value in $row)
					<td class="table-field-value">$value</td>
				#end
			</tr>
		#end
	</tbody>
</table>